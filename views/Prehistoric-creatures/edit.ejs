<form method="POST" action="/prehistoric_creatures/<%= creatureId %>/?_method=PUT">
    <label for="creatureType">Type:</label>
    <input type="text" name="type" value="<%= creature.type %>" id="creatureType"/>
    
    <label for="creatureImg_Url">Image URL:</label>
    <input type="text" name="img_url" value="<%= creature.img_url %>" id="creatureImg_Url"/>

    <button type="submit">Edit a Creature!</button>
</form>